{
  "name": "vanilla-hmr-redux-boilerplate",
  "version": "0.0.1",
  "description": "Simple boilerplate with hot module reload and redux/react",
  "scripts": {
    "clean": "rimraf build/",
    "build": "npm run lint && npm run clean && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "dev": "cross-env NODE_ENV=development node webpack/server.js",
    "start:prod": "npm run build && http-server build/ -p 9999 -o",
    "lint": "eslint src"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.7.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.1",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.5",
    "lodash": "^4.6.1",
    "node-sass": "^3.4.2",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.4.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "history": "^2.0.0-rc2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.1.1",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "redux-thunk": "^1.0.3"
  }
}
